<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".externalService.Emotional_profile">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/assertionlist"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/empty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/assertionlist"
        app:layout_constraintEnd_toEndOf="@+id/assertionlist"
        app:layout_constraintStart_toStartOf="@+id/assertionlist"
        app:layout_constraintTop_toTopOf="@+id/assertionlist"
        android:textSize="17.5sp"
        android:textColor="@color/buttonColor"
        />

</androidx.constraintlayout.widget.ConstraintLayout>